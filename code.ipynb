    "    return pd.DataFrame(applications_data)\n",
    "\n",
    "def clean_currency_values(df):\n",
    "    \"\"\"Clean currency values exactly as in the original code\"\"\"\n",
    "    currency_columns = [col for col in df.columns if any(keyword in col.lower() for keyword in \n",
    "                       ['salaire', 'cost_', 'financing_', 'amount', 'revenus', 'mensualite', \n",
    "                        'credit_', 'property_', 'revenu', 'value', 'debt', 'montant'])]\n",
    "    \n",
    "    for col in currency_columns:\n",
    "        if col in df.columns:\n",
    "            df[col] = df[col].astype(str).str.replace('€', '').str.replace(',', '.').str.replace(' ', '')\n",
    "            df[col] = pd.to_numeric(df[col], errors='coerce')\n",
    "    \n",
    "    return df\n",
    "\n",
    "def convert_dates(df):\n",
    "    \"\"\"Convert date columns exactly as in the original code\"\"\"\n",
    "    date_columns = [col for col in df.columns if any(keyword in col.lower() for keyword in \n",
    "                   ['date', 'created_at', 'updated_at'])]\n",
    "    \n",
    "    for col in date_columns:\n",
    "        if col in df.columns:\n",
    "            df[col] = pd.to_datetime(df[col], errors='coerce')\n",
    "    \n",
    "    return df\n",
    "\n",
    "def load_and_process_json_file(file_path):\n",
    "    \"\"\"Load and process JSON file exactly as in the original code\"\"\"\n",
    "    try:\n",
    "        with open(file_path, 'r', encoding='utf-8') as file:\n",
    "            json_data = json.load(file)\n",
    "        \n",
    "        df = extract_loan_applications_from_json(json_data)\n",
    "        df = clean_currency_values(df)\n",
    "        df = convert_dates(df)\n",
    "        \n",
    "        print(f\"Successfully processed {len(df)} loan applications\")\n",
    "        print(f\"DataFrame shape: {df.shape}\")\n",
    "        \n",
    "        return df\n",
    "    \n",
    "    except Exception as e:\n",
    "        print(f\"Error processing file: {str(e)}\")\n",
    "        return None\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    # First load the JSON file properly\n",
    "    try:\n",
    "        with open(\"new_ddp.json\", 'r', encoding='utf-8') as file:\n",
    "            json_data = json.load(file)\n",
    "        \n",
    "        # Make sure the data is in the expected format (list of dictionaries)\n",
    "        if not isinstance(json_data, list):\n",
    "            json_data = [json_data]  # Wrap in list if it's a single dictionary\n",
    "            \n",
    "        # Now process the data\n",
    "        df = extract_loan_applications_from_json(json_data)\n",
    "        df = clean_currency_values(df)\n",
    "        df = convert_dates(df)\n",
    "        \n",
    "        if df is not None and not df.empty:\n",
    "            print(\"\\nFirst row of processed data:\")\n",
    "            print(df.iloc[0])  # Show first row for verification\n",
    "            df.to_csv('loan_applications.csv', index=False)\n",
    "            print(\"\\nData saved to 'loan_applications.csv'\")\n",
    "        else:\n",
    "            print(\"No valid data was processed\")\n",
    "            \n",
    "    except Exception as e:\n",
    "        print(f\"Error loading or processing file: {str(e)}\")"
