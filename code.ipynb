    "def extract_loan_applications_from_json(json_data: List[Dict[str, Any]]) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    Extracts loan application data from JSON format exactly as in the original code.\n",
    "    Each row represents a complete loan application with all original features.\n",
    "    \"\"\"\n",
    "    \n",
    "    applications_data = []\n",
    "    \n",
    "    for app in json_data:\n",
    "        try:\n",
    "            metadata = app.get('metadata', {})\n",
    "            extracted = app.get('extracted_data', {})\n",
    "            \n",
    "            row_data = {}\n",
    "            \n",
    "            # ===== METADATA =====\n",
    "            row_data.update({\n",
    "                'application_id': metadata.get('id'),\n",
    "                'application_name': metadata.get('name'),\n",
    "                'simulation_id': metadata.get('simulation_id'),\n",
    "                'partenaire_bancaire_id': metadata.get('partenaire_bancaire_id'),\n",
    "                'interlocuteurs_id': metadata.get('interlocuteurs_id'),\n",
    "                'banque_id': metadata.get('banque_id'),\n",
    "                'created_at': metadata.get('created_at'),\n",
    "                'updated_at': metadata.get('updated_at'),\n",
    "                'commentaire': metadata.get('commentaire'),\n",
    "                'etape': metadata.get('etape'),\n",
    "                'date_envoie': metadata.get('date_envoie'),\n",
    "                'date_decision': metadata.get('date_decision'),\n",
    "                'propositions_id': metadata.get('propositions_id'),\n",
    "                'montant_credit_initio': metadata.get('montant_credit_initio'),\n",
    "                'type': metadata.get('type')\n",
    "            })\n",
    "            \n",
    "            # ===== HEADER INFO =====\n",
    "            header_info = extracted.get('header_info', {})\n",
    "            row_data.update({\n",
    "                'header_date': header_info.get('date'),\n",
    "                'reference_affaire': header_info.get('reference_affaire'),\n",
    "                'nom_affaire': header_info.get('nom_affaire')\n",
    "            })\n",
    "            \n",
    "            # ===== CONTACT INFORMATION =====\n",
    "            presenter_info = extracted.get('presenter_info', {})\n",
    "            company_address = presenter_info.get('company_address', [])\n",
    "            row_data.update({\n",
    "                'presenter_company_address_1': company_address[0] if len(company_address) > 0 else None,\n",
    "                'presenter_company_address_2': company_address[1] if len(company_address) > 1 else None,\n",
