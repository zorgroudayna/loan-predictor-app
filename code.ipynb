    "            # Co-borrower income\n",
    "            co_borrower_income = income_info.get('co_borrower', {})\n",
    "            row_data.update({\n",
    "                'co_borrower_salaire_mensuel': co_borrower_income.get('Salaire mensuel (sur 12 mois)'),\n",
